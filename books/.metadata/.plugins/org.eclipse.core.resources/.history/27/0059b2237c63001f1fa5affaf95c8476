package hw;

class Animal {
    private String name;

    // 생성자
    public Animal(String name) {
        this.name = name;
    }

    // name 필드의 값을 반환하는 메서드
    public String getName() {
        return name;
    }
}

class Dog extends Animal {
    // 생성자
    public Dog(String name) {
        super(name); // 부모 클래스의 생성자 호출
    }

    // Dog 클래스의 bark 메서드
    public void bark() {
        System.out.println(getName() + "가 짖습니다: 멍멍!");
    }
}

public class HW01 {
    public static void main(String[] args) {
        Dog dog = new Dog("바우와우"); // Dog 객체 생성
        dog.bark(); // bark 메서드 호출
    }
}
