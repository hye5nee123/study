package hw;

class Dog extends Animal {
    public Dog(String name) {
        super(name); // 부모 클래스의 생성자 호출
    }

    public void bark() {
        System.out.println(getName() + "가 짖습니다: 멍멍!");
    }
}

public class Dog {
    public static void main(String[] args) {
        Dog dog = new Dog("바우와우");
        dog.bark(); // '바우와우가 짖습니다: 멍멍!' 출력
    }
}